{
    "name": "Trojan-Go",
    "description": "Deploy Trojan-Go to Heroku.",
    "keywords": [
        "Trojan-Go"
    ],
    "env": {
        "CADDYIndexPage": {
            "description": "自定义CADDY部署完成后点击View的显示页面",
            "value": "https://github.com/AYJCSGM/mikutap/archive/master.zip"
        },
        "CONFIGCADDY": {
            "description": "自定义网络地址读取配置运行CADDY，不熟悉CADDY配置的请勿修改",
            "value": "https://raw.githubusercontent.com/waloyn/trojan-go/main/Caddyfile"
        },
        "REMOTE_ADDR": {
            "description":"远程地址",
            "value": "example.com"
        },
        "REMOTE_PORT": {
            "description":"远程端口",
            "value": "443"
        },
        "TROJAN_PASSWORD": {
            "description":"TROJAN-GO密码",
            "value": ""
        },
        "WEBSOCKET_PATH": {
            "description":"WS路径",
            "value": "/trojango"
        },
        "SHADOWSOCKS_ENABLED": {
            "description":"是否启用SS AEAD加密",
            "value": "true"
        },
        "SHADOWSOCKS_METHOD": {
            "description":"加密方式,合法值AES-128-GCM;AES-256-GCM;CHACHA20-IETF-POLY1305",
            "value": "AES-128-GCM"
        },
        "SHADOWSOCKS_PASSWORD": {
            "description":"SS AEAD加密密码",
            "value": ""
        }
    },
    "website": "https://github.com/waloyn/heroku-trojan-go",
    "repository": "https://github.com/waloyn/heroku-trojan-go",
    "stack": "container"
}
